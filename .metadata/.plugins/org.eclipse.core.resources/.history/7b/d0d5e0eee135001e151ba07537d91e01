package ConditionalOperator;

public class Conditioanal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 10 ;
		int b = 20;
		
		if (a == b) {
			System.out.println("I am vimal patel");
			}
		
		else {
			System.out.println("I am not vimal patel");
		}
		
	}

}
